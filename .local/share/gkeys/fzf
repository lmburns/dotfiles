esc        : abort
ctrl-c     : abort
ctrl-q     : abort
ctrl-g     : cancel
ctrl-j     : down
ctrl-k     : up
home       : beginning-of-line
end        : end-of-line
ctrl-s     : beginning-of-line
ctrl-e     : end-of-line
alt-x      : unix-line-discard
alt-c      : unix-word-rubout
alt-d      : kill-word
ctrl-h     : backward-delete-char
alt-bs     : backward-kill-word
ctrl-w     : backward-kill-word
alt-a      : toggle-all
ctrl-alt-a : toggle-all+accept
alt-s      : toggle-sort
ctrl-r     : clear-selection
page-up    : prev-history
page-down  : next-history
alt-(      : prev-history
alt-)      : next-history
alt-left   : first
alt-right  : last
alt-up     : prev-selected
alt-down   : next-selected
ctrl-u     : half-page-up
ctrl-d     : half-page-down
ctrl-alt-u : page-up
ctrl-alt-d : page-down
alt-o      : replace-query+print-query
ctrl-/     : jump
?          : toggle-preview
alt-[      : toggle-preview
alt-]      : change-preview-window(70%|45%,down,border-top|45%,up,border-bottom|)+show-preview
alt-w      : toggle-preview-wrap
ctrl-b     : preview-up
ctrl-f     : preview-down
ctrl-alt-b : preview-page-up
ctrl-alt-f : preview-page-down
alt-e      : become($EDITOR {+})
alt-b      : become(bat --paging=always -f {+})
ctrl-y     : execute-silent(xsel --trim -b <<< {+})
ctrl-]     : preview(bat --color=always -l bash \"$XDG_DATA_HOME/gkeys/fzf\")
alt-/      : unbind(?)
ctrl-\\    : rebind(?)
