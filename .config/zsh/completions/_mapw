_mapw() {
  local arguments

  arguments=(
    '-h[display this help]'
    '-m[map (show) wid]'
    '-t[toggle widâ€™s visibility]'
    '-u[unmap (hide) wid]'
    '*:WINID:'
  )

  _arguments -s $arguments
}

_mapw "$@"
