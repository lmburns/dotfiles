  {-U,--uuid}'[convert UUID to device name]'
  {-p,--probe}'[low-level superblocks probing (bypass cache)]'
  {-i,--info}'[gather information about I/O limits]'
  {-H,--hint}'[set hint for probing function]'
  {-S,--size}'[overwrite device size]'
  {-O,--offset}'[probe at the given offset]'
  {-u,--usages}'[filter by "usage" (e.g. -u filesystem,raid)]'
  {-n,--match-types}'[filter by filesystem type (e.g. -n vfat,ext3)]'
