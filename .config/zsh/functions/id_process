  # for (( i=0; i<=${#usernames}; i++ )) { final[${usernames[(e)${uids[$i]}]}]="${pids[$i]}" }
  #
  # print -raC 2 -- ${(@kv)final}
  # print -Prl -- ${${(@k)final}//(#b)(*)/%F{1}$match[1] %F{2}$ass[$match[1]]}
}

id_process

